version: 0.1
log:
  fields:
    service: registry
storage:
# We're explicitly removing any "cache:" section because of issues with it
    filesystem:
        rootdirectory: /var/lib/registry
    delete:
        enabled: true
http:
    addr: {{ dci_base_ip|default(ansible_default_ipv4.address) }}:{{ dci_registry_port|default(5000) }}
